<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:DynamicDataTraining.ViewModel.ViewModelsFolder.Filters"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DynamicDataTraining.UI.Views.Filters.AbsencesRangeFilterView"
             x:DataType="vm:AbsencesRangeFilterViewModel">
  <StackPanel Orientation="Horizontal">
    <TextBlock Text="Min:" />
    <NumericUpDown
         x:Name="PageSizeNumeric"
         Value="{Binding Min, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
         Minimum="0"
         Width="120"
         AllowSpin="True"
         HorizontalContentAlignment="Center"
         VerticalAlignment="Center"
         FormatString="F0"/>
    <TextBlock Text="Max:" />
    <NumericUpDown
         x:Name="PageSizeNumeric"
         Value="{Binding Max, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
         Minimum="0"
         Width="120"
         AllowSpin="True"
         HorizontalContentAlignment="Center"
         VerticalAlignment="Center"
         FormatString="F0"/>
  </StackPanel>
</UserControl>
